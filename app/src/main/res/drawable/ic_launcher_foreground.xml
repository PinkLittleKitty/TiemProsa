<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <group android:scaleX="0.7" android:scaleY="0.7" android:pivotX="54" android:pivotY="54">
        
        <!-- Main "T" shape that doubles as a clock -->
        <group android:translateX="54" android:translateY="54">
            
            <!-- The "T" horizontal bar -->
            <path
                android:fillColor="#FFFFFF"
                android:pathData="M-20,-6 L20,-6 L20,6 L-20,6 Z" />
            
            <!-- The "T" vertical bar (also acts as 12 o'clock hand) -->
            <path
                android:fillColor="#FFFFFF"
                android:pathData="M-4,-6 L4,-6 L4,25 L-4,25 Z" />
            
            <!-- Quirky clock dots at 3, 6, 9 positions -->
            <!-- 3 o'clock -->
            <path
                android:fillColor="#FF6B35"
                android:pathData="M18,0m-3,0a3,3 0,1 1,6 0a3,3 0,1 1,-6 0" />
            
            <!-- 6 o'clock -->
            <path
                android:fillColor="#FF6B35"
                android:pathData="M0,18m-3,0a3,3 0,1 1,6 0a3,3 0,1 1,-6 0" />
            
            <!-- 9 o'clock -->
            <path
                android:fillColor="#FF6B35"
                android:pathData="M-18,0m-3,0a3,3 0,1 1,6 0a3,3 0,1 1,-6 0" />
            
            <!-- Small book icon in the bottom right of the T -->
            <group android:translateX="12" android:translateY="15" android:scaleX="0.6" android:scaleY="0.6">
                <path
                    android:fillColor="#FFD54F"
                    android:pathData="M-4,-3 L4,-3 L4,3 L-4,3 Z" />
                <path
                    android:fillColor="#FFC107"
                    android:pathData="M-3,-2 L3,-2 L3,-1 L-3,-1 Z" />
                <path
                    android:fillColor="#FFC107"
                    android:pathData="M-3,0 L3,0 L3,1 L-3,1 Z" />
            </group>
            
        </group>
        
    </group>
</vector>